<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="504.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.bsommerfeld.randomizer.ui.view.controller.SettingsViewController">
    <children>
        <HBox prefHeight="360.0" prefWidth="876.0" styleClass="settings-container" AnchorPane.bottomAnchor="12.0" AnchorPane.leftAnchor="12.0" AnchorPane.rightAnchor="12.0" AnchorPane.topAnchor="12.0">
            <children>
                <VBox prefHeight="344.0" prefWidth="112.0" styleClass="settings-selection" HBox.hgrow="NEVER">
                    <children>
                        <HBox styleClass="settings-selection" HBox.hgrow="ALWAYS">
                            <children>
                        <VBox styleClass="settings-selection-container" VBox.vgrow="ALWAYS">
                           <children>
                                      <ToggleButton fx:id="generalToggleButton" mnemonicParsing="false" styleClass="settings-options" text="General" />
                                      <ToggleButton fx:id="machineLearningToggleButton" mnemonicParsing="false" styleClass="settings-options" text="Machine Learning" />
                           </children>
                        </VBox>
                            </children>
                        </HBox>
                    </children>
                </VBox>
                <HBox prefHeight="344.0" prefWidth="752.0" HBox.hgrow="ALWAYS">
                    <children>
                        <GridPane fx:id="contentPane" prefHeight="344.0" prefWidth="728.0" HBox.hgrow="ALWAYS">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            </rowConstraints>
                        </GridPane>
                    </children>
               <padding>
                  <Insets left="12.0" />
               </padding>
                </HBox>
            </children>
        </HBox>
    </children>
</AnchorPane>
